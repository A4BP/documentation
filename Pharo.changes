te implementations!!PRSubCommandLineHandler methodsFor: 'activation' stamp: 'BenjaminVanRyseghem 1/9/2014 10:41'!activate	| confFilename |	self activateHelp		ifTrue: [ ^ self ].	self		optionAt: 'configurationFile'		ifPresent: [ :file | confFilename := file ]		ifAbsent: [ confFilename := 'pillar.conf' ].	self optionAt: 'baseDirectory' ifPresent: [ :dir | baseDirectory := dir asFileReference ].	self createConfiguration: confFilename! !!PRSubCommandLineHandler methodsFor: 'accessing' stamp: 'BenjaminVanRyseghem 1/9/2014 11:01'!baseDirectory	^ baseDirectory ifNil: [ (OSEnvironment default getEnv: 'PWD') asFileReference ]! !!PRSubCommandLineHandler methodsFor: 'activation' stamp: 'CyrilFerlicot 4/1/2015 18:20'!createConfiguration: confFilename	| confFile |	confFile := self baseDirectory resolve: confFilename.	configuration := confFile exists		ifTrue: [ self readConfigurationFromFile: confFile ]		ifFalse: [ PRCocoonConfiguration new ].! !!PRSubCommandLineHandler methodsFor: 'error handling' stamp: 'DamienCassou 12/11/2013 16:34'!error: aString	self stderr << aString; lf.	self exitFailure! !!PRSubCommandLineHandler methodsFor: 'error handling' stamp: 'DamienCassou 1/8/2014 11:13'!errorMissingParameter: aParameterName	self error: 'Missing required parameter --', aParameterName ! !!PRSubCommandLineHandler methodsFor: 'activation' stamp: 'CyrilFerlicot 4/1/2015 18:20'!readConfigurationFromFile: confFile	^ [ (PRCocoonConfigurationInterpreter baseDirectory: self baseDirectory) buildFromFile: confFile ]		on: STONReaderError		do: [ :error | self exitFailure: error messageText ]! !!PRExportCommandLineHandler class methodsFor: 'accessing' stamp: 'DamienCassou 12/11/2013 15:37'!commandName	^ 'export'! !!PRExportCommandLineHandler methodsFor: 'activation' stamp: 'BenjaminVanRyseghem 1/9/2014 11:06'!activate	super activate.	subconfName isEmptyOrNil ifTrue: [ self errorMissingParameter: 'to' ].	self optionAt: 'outputFile' ifPresent: [ :file | 